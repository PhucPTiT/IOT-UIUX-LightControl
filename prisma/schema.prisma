generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model DataLog {
  id String @id @default(uuid())
  temp String
  humidity String
  brightness String
  time DateTime
}

model ControlLog{
  id String @id @default(uuid())
  light Boolean
  fan Boolean
  time DateTime
}